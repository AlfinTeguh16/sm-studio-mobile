{
  "expo": {
    "name": "SM Studio",
    "slug": "mua-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smstudio.mua",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Lokasi dipakai untuk menampilkan MUA di sekitar Anda."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"],
  
      "package": "com.smstudio.mua"
    },
    "scheme": "smstudio",
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      "expo-secure-store"
    ]
  }
}
